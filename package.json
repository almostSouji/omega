{
	"name": "omega-rules",
	"version": "0.4.3",
	"description": "SIEM rules for JS objects",
	"author": "almostSouji <timoqueezle@gmail.com>",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist --strip-leading-paths",
		"build": "yarn build:clean && yarn build:check && yarn build:esm",
		"start": "node dist/index.js",
		"start:dev": "npm run build && npm run dev",
		"dev": "node --enable-source-maps dist/index.js",
		"test": "yarn build && vitest run",
		"test:watch": "yarn build && vitest watch",
		"prepare": "is-ci || husky install",
		"prepublish": "tsc",
		"lint": "yarn biome lint",
		"format": "yarn biome format --write"
	},
	"dependencies": {
		"chevrotain": "^11.0.3",
		"octokit": "^4.0.2",
		"re2": "^1.22.3",
		"readdirp": "^4.0.2",
		"yaml": "^2.6.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-angular": "^18.4.3",
		"@swc/cli": "^0.5.2",
		"@swc/core": "^1.10.1",
		"@swc/helpers": "^0.5.15",
		"@types/node": "^22.10.2",
		"del-cli": "^6.0.0",
		"lefthook": "^2.0.13",
		"rimraf": "^6.0.1",
		"typescript": "^5.7.2",
		"vitest": "^4.0.16"
	},
	"engines": {
		"node": ">=24.0.0"
	},
	"packageManager": "yarn@4.5.3"
}
